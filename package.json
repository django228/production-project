{
  "name": "production-project",
  "version": "1.0.0",
  "description": "React TypeScript production project",
 "private": true,
  "homepage": "https://django228.github.io/production-project",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "build:github": "cross-env GITHUB_PAGES=true webpack --env mode=production",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" --config config/eslint/.eslintrc.js",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --config config/eslint/.eslintrc.js --fix",
    "lint:scss": "stylelint \"**/*.scss\" --config config/stylelint/.stylelintrc.js",
    "lint:scss:fix": "stylelint \"**/*.scss\" --config config/stylelint/.stylelintrc.js --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,css,json}\" --config config/prettier/.prettierrc",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,css,json}\" --config config/prettier/.prettierrc",
    "test": "jest",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^7.2.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.17",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-webpack5": "^9.1.5",
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-i18next-extract": "^0.8.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-i18next": "^5.2.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^9.1.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^2.8.0",
    "react-refresh": "^0.12.0",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "storybook": "^9.1.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^1.6.0",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^1.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
